server {
	server_name youpi
	listen 0.0.0.0:3007

	root ./pages
	index index.html

	allow_methods POST GET

	location /put_test {
		allow_methods POST GET
		root ./YoupiBanane/put_here
		autoindex on
	}

	location /post_body {
		allow_methods POST DELETE GET
		client_body_buffer_size 100
	}

	location /directory {
		allow_methods GET
		root YoupiBanane/
		index youpi.bad_extension
	}

	location *.bla {
		allow_methods POST
		cgi_pass test_mac/macos_cgi_tester
	}
}
